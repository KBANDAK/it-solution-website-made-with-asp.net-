<style>
    /* CSS Custom Properties for better maintainability */
    :root {
        --primary-color: #00FFF7;
        --bg-primary: #0d1117;
        --bg-secondary: #141d29;
        --bg-tertiary: #171f2c;
        --text-primary: #ffffff;
        --text-secondary: rgba(255, 255, 255, 0.9);
        --border-color: rgba(0, 255, 247, 0.1);
        --border-color-hover: rgba(0, 255, 247, 0.3);
        --shadow-primary: rgba(0, 0, 0, 0.3);
        --shadow-hover: rgba(0, 0, 0, 0.4);
        --transition-smooth: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
        --transition-fast: all 0.2s ease;
        --border-radius-sm: 8px;
        --border-radius-md: 12px;
        --border-radius-lg: 15px;
        --border-radius-xl: 20px;
        --spacing-xs: 8px;
        --spacing-sm: 12px;
        --spacing-md: 20px;
        --spacing-lg: 30px;
        --spacing-xl: 40px;
        --spacing-2xl: 60px;
        --spacing-3xl: 80px;
    }

    /* Reset and Base Styles */
    * {
        box-sizing: border-box;
    }

    body {
        background-color: var(--bg-primary);
        color: var(--text-primary);
        margin: 0;
        padding: 0;
        overflow-x: hidden;
        font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', 'Roboto', 'Oxygen', 'Ubuntu', 'Cantarell', sans-serif;
        line-height: 1.6;
        -webkit-font-smoothing: antialiased;
        -moz-osx-font-smoothing: grayscale;
    }

    img {
        max-width: 100%;
        height: auto;
        display: block;
    }

    .container {
        max-width: 1200px;
        width: 100%;
        padding: 0 clamp(16px, 4vw, 32px);
        margin: 0 auto;
        position: relative;
        z-index: 2;
    }

    /* Hero Section */
    .hero-section {
        position: relative;
        min-height: clamp(400px, 80vh, 600px);
        overflow: hidden;
        display: flex;
        align-items: center;
        justify-content: center;
        width: 100%;
        margin: 0;
        padding: clamp(40px, 8vh, 80px) 0;
        background-color: var(--bg-primary);
    }

    .hero-background {
        position: absolute;
        inset: 0;
        z-index: 0;
    }

    .grid-overlay {
        position: absolute;
        inset: 0;
        background-image:
            linear-gradient(var(--border-color) 1px, transparent 1px),
            linear-gradient(90deg, var(--border-color) 1px, transparent 1px);
        background-size: clamp(30px, 5vw, 40px) clamp(30px, 5vw, 40px);
        opacity: 0;
        animation: fadeInGrid 2s ease forwards 0.5s;
    }

    .particles-container {
        position: absolute;
        inset: 0;
    }

    /* Enhanced Tech Elements with Better Responsive Behavior */
    .tech-element {
        position: absolute;
        opacity: 0;
        z-index: 1;
        animation: fadeIn 1.5s ease forwards 0.5s;
        will-change: transform, opacity;
    }

    .tech-hexagon {
        width: clamp(200px, 25vw, 300px);
        height: clamp(173px, 21.6vw, 260px);
        background: linear-gradient(145deg, rgba(0, 255, 247, 0.05), transparent);
        clip-path: polygon(50% 0%, 100% 25%, 100% 75%, 50% 100%, 0% 75%, 0% 25%);
        top: 10%;
        right: 10%;
        animation: pulseHex 15s ease-in-out infinite;
    }

    .tech-circuit {
        width: clamp(120px, 15vw, 200px);
        height: clamp(180px, 22.5vw, 300px);
        background-image:
            linear-gradient(var(--border-color) 1px, transparent 1px),
            linear-gradient(90deg, var(--border-color) 1px, transparent 1px);
        background-size: 20px 20px;
        bottom: 10%;
        left: 5%;
        transform: perspective(500px) rotateY(30deg);
        animation: fadeIn 2s ease forwards 1s, floatElement 8s ease-in-out infinite;
    }

    .tech-pulse {
        width: clamp(100px, 12.5vw, 150px);
        height: clamp(100px, 12.5vw, 150px);
        border-radius: 50%;
        background: radial-gradient(circle, rgba(0, 255, 247, 0.1) 0%, transparent 70%);
        top: 60%;
        right: 20%;
        animation: pulse 4s infinite;
    }

    .hero-content {
        text-align: center;
        padding: 0 var(--spacing-md);
        max-width: 800px;
        margin: 0 auto;
        z-index: 10;
    }

    .hero-title {
        font-family: "Share Tech Mono", monospace;
        font-size: clamp(28px, 6vw, 48px);
        font-weight: 700;
        color: var(--primary-color);
        margin-bottom: var(--spacing-md);
        text-shadow: 0 0 15px rgba(0, 255, 247, 0.5);
        line-height: 1.2;
    }

    .hero-subtitle {
        font-size: clamp(16px, 3vw, 20px);
        color: var(--text-primary);
        margin-bottom: var(--spacing-xl);
        line-height: 1.5;
        max-width: 600px;
        margin-left: auto;
        margin-right: auto;
    }

    .tech-badge {
        display: inline-block;
        padding: var(--spacing-xs) var(--spacing-sm);
        border: 1px solid var(--border-color-hover);
        background-color: rgba(0, 255, 247, 0.05);
        border-radius: var(--border-radius-xl);
        position: relative;
        overflow: hidden;
    }

    .badge-text {
        font-family: "Share Tech Mono", monospace;
        color: var(--primary-color);
        font-size: clamp(12px, 2vw, 14px);
        font-weight: 600;
        letter-spacing: 1px;
        position: relative;
        z-index: 1;
    }

    .badge-glow {
        position: absolute;
        width: 10px;
        height: 100%;
        background: linear-gradient(90deg, transparent, rgba(0, 255, 247, 0.5), transparent);
        top: 0;
        left: -20px;
        transform: skewX(-20deg);
        animation: badgeGlow 4s infinite;
    }

    /* Enhanced Section Styles */
    .mission-section,
    .team-section,
    .why-choose-section,
    .cta-section {
        position: relative;
        padding: clamp(40px, 8vh, 80px) 0;
        background-color: var(--bg-primary);
        overflow: hidden;
    }

    .section-header {
        position: relative;
        text-align: center;
        margin-bottom: clamp(40px, 8vw, 60px);
        z-index: 2;
    }

    .section-title {
        font-family: "Share Tech Mono", monospace;
        font-size: clamp(24px, 5vw, 36px);
        font-weight: 700;
        color: var(--primary-color);
        margin-bottom: 15px;
        text-transform: uppercase;
        letter-spacing: 2px;
        position: relative;
        line-height: 1.3;
    }

    .title-underline {
        display: flex;
        align-items: center;
        justify-content: center;
        gap: 10px;
        margin: 0 auto;
        width: min(200px, 80vw);
    }

    .line-segment {
        height: 2px;
        background: linear-gradient(90deg, rgba(0, 255, 247, 0), rgba(0, 255, 247, 0.7));
        flex-grow: 1;
    }

    .line-segment:last-child {
        background: linear-gradient(90deg, rgba(0, 255, 247, 0.7), rgba(0, 255, 247, 0));
    }

    .line-dot {
        width: 8px;
        height: 8px;
        background-color: var(--primary-color);
        border-radius: 50%;
        position: relative;
        flex-shrink: 0;
    }

    .line-dot::after {
        content: '';
        position: absolute;
        width: 16px;
        height: 16px;
        border: 1px solid var(--border-color-hover);
        border-radius: 50%;
        top: -50%;
        left: -50%;
        transform: translate(-50%, -50%);
        animation: pulse 2s infinite;
    }

    /* Mission Section */
    .mission-content {
        display: flex;
        flex-direction: column;
        gap: clamp(30px, 6vw, 50px);
    }

    .mission-card {
        background: linear-gradient(145deg, var(--bg-secondary), var(--bg-primary));
        border: 1px solid var(--border-color);
        border-radius: var(--border-radius-lg);
        padding: clamp(25px, 5vw, 40px);
        text-align: center;
        position: relative;
        box-shadow: 0 10px 30px var(--shadow-primary);
        max-width: 800px;
        margin: 0 auto;
        transition: var(--transition-smooth);
    }

    .mission-card:hover {
        transform: translateY(-5px);
        box-shadow: 0 15px 40px var(--shadow-hover);
        border-color: var(--border-color-hover);
    }

    .card-icon {
        display: flex;
        align-items: center;
        justify-content: center;
        margin-bottom: 25px;
        position: relative;
    }

    .icon-ring {
        position: absolute;
        width: 70px;
        height: 70px;
        border: 2px solid rgba(0, 255, 247, 0.2);
        border-radius: 50%;
        animation: pulseRing 3s infinite;
    }

    .mission-title {
        font-family: "Share Tech Mono", monospace;
        color: var(--primary-color);
        font-size: clamp(18px, 4vw, 24px);
        margin-bottom: var(--spacing-md);
        line-height: 1.3;
    }

    .mission-text {
        color: var(--text-primary);
        line-height: 1.8;
        font-size: clamp(14px, 2.5vw, 16px);
        max-width: 700px;
        margin: 0 auto;
    }

    /* Tech Corners */
    .tech-corners .corner {
        position: absolute;
        width: 20px;
        height: 20px;
        border-color: rgba(0, 255, 247, 0.2);
        border-style: solid;
        border-width: 0;
        opacity: 0;
        animation: fadeIn 1s ease forwards 0.5s;
    }

    .corner.top-left {
        top: 15px;
        left: 15px;
        border-top-width: 2px;
        border-left-width: 2px;
    }

    .corner.top-right {
        top: 15px;
        right: 15px;
        border-top-width: 2px;
        border-right-width: 2px;
    }

    .corner.bottom-left {
        bottom: 15px;
        left: 15px;
        border-bottom-width: 2px;
        border-left-width: 2px;
    }

    .corner.bottom-right {
        bottom: 15px;
        right: 15px;
        border-bottom-width: 2px;
        border-right-width: 2px;
    }

    /* Enhanced Grid Layouts */
    .values-container {
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(min(280px, 100%), 1fr));
        gap: clamp(20px, 4vw, 30px);
        justify-items: center;
    }

    .value-item {
        width: 100%;
        max-width: 350px;
        background: linear-gradient(145deg, #111922, #0a0e14);
        border: 1px solid var(--border-color);
        border-radius: var(--border-radius-md);
        padding: clamp(20px, 4vw, 30px);
        text-align: center;
        position: relative;
        transition: var(--transition-smooth);
        backdrop-filter: blur(10px);
    }

    .value-item:hover {
        transform: translateY(-5px);
        box-shadow: 0 15px 30px var(--shadow-hover);
        border-color: var(--border-color-hover);
    }

    .value-icon {
        display: flex;
        align-items: center;
        justify-content: center;
        margin-bottom: var(--spacing-md);
        position: relative;
    }

    .value-item h4 {
        font-family: "Share Tech Mono", monospace;
        color: var(--primary-color);
        font-size: clamp(16px, 3vw, 18px);
        margin-bottom: 15px;
        line-height: 1.3;
    }

    .value-item p {
        color: var(--text-primary);
        line-height: 1.6;
        font-size: clamp(13px, 2.5vw, 14px);
    }

    /* Team Section */
    .team-grid {
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(min(300px, 100%), 1fr));
        gap: clamp(25px, 5vw, 40px);
        justify-items: center;
    }

    .team-card {
        width: 100%;
        max-width: 350px;
