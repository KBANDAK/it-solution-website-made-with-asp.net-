@{
    ViewBag.Title = "About";
}
<!--About Hero-Section -->
<section class="hero-section">
    <div class="hero-background">
        <div class="grid-overlay"></div>
        <div class="particles-container" id="about-particles-js"></div>
    </div>

    <!-- Tech elements -->
    <div class="tech-element tech-hexagon"></div>
    <div class="tech-element tech-circuit"></div>
    <div class="tech-element tech-pulse"></div>

    <div class="container">
        <div class="hero-content">
            <h1 class="hero-title animate__animated animate__fadeIn">About SecuDev</h1>
            <p class="hero-subtitle animate__animated animate__fadeIn animate__delay-1s">We build secure, future-proof tech with passion and precision.</p>
            <div class="tech-badge animate__animated animate__fadeIn animate__delay-2s">
                <span class="badge-text">EST. 2021</span>
                <div class="badge-glow"></div>
            </div>
        </div>
    </div>
</section>

<!--About Mission and Goals Section -->
<section class="mission-section">
    <div class="container">
        <div class="section-header">
            <h2 class="section-title animate__animated animate__fadeIn">Our Mission</h2>
            <div class="title-underline">
                <span class="line-segment"></span>
                <span class="line-dot"></span>
                <span class="line-segment"></span>
            </div>
        </div>

        <div class="mission-content">
            <div class="mission-card animate__animated animate__fadeInUp">
                <div class="card-icon">
                    <img src="~/Content/Images/Icons/mission.png" alt="Mission Icon" width="48" height="48" />
                    <div class="icon-ring"></div>
                </div>
                <h3 class="mission-title">Securing Digital Futures, Empowering Innovation</h3>
                <p class="mission-text">Our mission is to empower organizations with cutting-edge security, development, and network solutions that protect digital assets while enabling growth and innovation without compromise.</p>
                <div class="tech-corners">
                    <span class="corner top-left"></span>
                    <span class="corner top-right"></span>
                    <span class="corner bottom-left"></span>
                    <span class="corner bottom-right"></span>
                </div>
            </div>

            <div class="values-container">
                <div class="value-item animate__animated animate__fadeInUp animate__delay-1s">
                    <div class="value-icon">
                        <img src="~/Content/Images/Icons/encrypted (1).png" alt="Security Icon" width="36" height="36" />
                    </div>
                    <h4>Security First</h4>
                    <p>We build with security as the foundation — ensuring protection at every layer of your digital ecosystem.</p>
                </div>

                <div class="value-item animate__animated animate__fadeInUp animate__delay-2s">
                    <div class="value-icon">
                        <img src="~/Content/Images/Icons/code (1).png" alt="Development Icon" width="36" height="36" />
                    </div>
                    <h4>Engineering Excellence</h4>
                    <p>Clean code. Scalable architecture. Seamless experiences. We take pride in development done right.</p>
                </div>

                <div class="value-item animate__animated animate__fadeInUp animate__delay-3s">
                    <div class="value-icon">
                        <img src="~/Content/Images/Icons/innovation.png" alt="Innovation Icon" width="36" height="36" />
                    </div>
                    <h4>Innovative Thinking</h4>
                    <p>We stay ahead through R&D, embracing new technologies that give our clients a competitive edge.</p>
                </div>

                <div class="value-item animate__animated animate__fadeInUp animate__delay-4s">
                    <div class="value-icon">
                        <img src="~/Content/Images/Icons/global-network.png" alt="Network Icon" width="36" height="36" />
                    </div>
                    <h4>Infrastructure Reliability</h4>
                    <p>From cloud architecture to on-site networking — we design systems that are resilient and efficient.</p>
                </div>

                <div class="value-item animate__animated animate__fadeInUp animate__delay-5s">
                    <div class="value-icon">
                        <img src="~/Content/Images/Icons/group.png" alt="Collaboration Icon" width="36" height="36" />
                    </div>
                    <h4>Client-Centered Partnership</h4>
                    <p>We work closely with our clients — listening, adapting, and delivering solutions that match their goals.</p>
                </div>
            </div>
        </div>
    </div>

    <!-- Background elements -->
    <div class="tech-blob"></div>
    <div class="tech-grid vertical"></div>
</section>

